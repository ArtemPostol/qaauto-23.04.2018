<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Linkedin LogIn">

    <test name="Login Tests Chrome us">
        <parameter name="browserType" value="chrome "/>
        <parameter name="envURL" value="https://us.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
    </test>

    <test name="Login Tests Firefox us">
        <parameter name="browserType" value="firefox"/>
        <parameter name="envURL" value="https://us.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
    </test>

    <test name="Login Tests Chrome uk">
    <parameter name="browserType" value="chrome"/>
    <parameter name="envURL" value="https://uk.linkedin.com/" />
    <classes>
        <class name="test.LinkedinLoginTest" />
    </classes>
</test>

    <test name="Login Tests Firefox uk">
        <parameter name="browserType" value="firefox"/>
        <parameter name="envURL" value="https://uk.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
    </test>

    <test name="Login Tests Chrome">
        <parameter name="browserType" value="chrome"/>
        <parameter name="envURL" value="https://www.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
    </test>

    <test name="Login Tests Firefox">
        <parameter name="browserType" value="firefox"/>
        <parameter name="envURL" value="https://www.linkedin.com/" />
        <classes>
            <class name="test.LinkedinLoginTest" />
        </classes>
    </test>



</suite>
